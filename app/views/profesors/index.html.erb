<div class="panel panel-default">
  <div class="panel-heading">Profesores</div>

<table>
  <thead>
    <tr>
      <table class="table">

      <th>Nombre</th>
      <th>Apellido</th>
      <th>Correo</th>
      <th>Especialidad</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profesors.each do |profesor| %>
      <tr>
        <td><%= profesor.nombreprof %></td>
        <td><%= profesor.approf %></td>
        <td><%= profesor.correo %></td>
        <td><%= profesor.especialidad.nombre %></td>
        <td><%= link_to 'Show', profesor %></td>
        <td><%= link_to 'Edit', edit_profesor_path(profesor) %></td>
        <td><%= link_to 'Destroy', profesor, method: :delete, data: { confirm: 'Seguro que quieres eliminar?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>

<br>

<center><%= link_to 'Agregar Profesor', new_profesor_path %></center>
<center><%= link_to 'Atras', inicio_index_path %></center>
