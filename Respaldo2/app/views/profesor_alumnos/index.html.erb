<p id="notice"><%= notice %></p>

<h1>Listing Profesor Alumnos</h1>

<table>
  <thead>
    <tr>
      <th>Profesor</th>
      <th>Alumno</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @profesor_alumnos.each do |profesor_alumno| %>
      <tr>
        <td><%= profesor_alumno.profesor %></td>
        <td><%= profesor_alumno.alumno %></td>
        <td><%= link_to 'Show', profesor_alumno %></td>
        <td><%= link_to 'Edit', edit_profesor_alumno_path(profesor_alumno) %></td>
        <td><%= link_to 'Destroy', profesor_alumno, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

